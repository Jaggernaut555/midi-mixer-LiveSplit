{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "id": "ca.jaggernaut.midi-mixer.livesplit",
  "name": "LiveSplit",
  "type": "node",
  "version": "1.0.1",
  "author": "Jaggernaut",
  "main": "lib/index.js",
  "icon": "livesplit.png",
  "settings": {
    "livesplitip": {
      "label": "LivesSplit IP",
      "type": "text",
      "fallback": "127.0.0.1"
    },
    "livesplitport": {
      "label": "LivesSplit Port",
      "type": "text",
      "fallback": "16834"
    }
  }
}